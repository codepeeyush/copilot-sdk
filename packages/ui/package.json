{
  "name": "@yourgpt/copilot-sdk-ui",
  "version": "0.1.0",
  "description": "Pre-built UI components for YourGPT Copilot SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@yourgpt/copilot-sdk-core": "workspace:*",
    "@yourgpt/copilot-sdk-react": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "lucide-react": "^0.561.0",
    "marked": "^17.0.1",
    "react-markdown": "^9.1.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.20.0",
    "tailwind-merge": "^3.4.0",
    "use-stick-to-bottom": "^1.1.1"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourgpt/copilot-sdk.git",
    "directory": "packages/ui"
  },
  "keywords": [
    "yourgpt",
    "copilot",
    "ai",
    "chatbot",
    "react",
    "ui",
    "components"
  ],
  "sideEffects": [
    "*.css"
  ]
}
